<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>org.codice</groupId>
	<artifactId>thirdparty</artifactId>
	<packaging>pom</packaging>
	<name>Codice Thirdparty Bundles</name>
	<version>1.0.0</version>

	<modules>
		<module>picocontainer</module>
		<module>geotools-suite</module>
		<module>gt-opengis</module>
		<module>jts</module>
		<module>commons-httpclient-3.0.1</module>
		<module>commons-httpclient-3.1</module>
		<module>lucene-core-3.0.2</module>
		<module>cas-client-core-3.1.10</module>
		<module>ogc-filter-v_1_1_0-schema</module>
		<module>ffmpeg</module>
		<module>tika-bundle</module>
	</modules>

	<scm>
		<url>https://github.com/codice/thirdparty.git</url>
		<connection>scm:git:git://github.com/codice/thirdparty.git</connection>
		<developerConnection>scm:git:git://github.com/codice/thirdparty.git</developerConnection>
		<tag>HEAD</tag>
	</scm>

	<distributionManagement>
		<snapshotRepository>
			<id>snapshots</id>
			<url>${snapshots.repository.url}</url>
		</snapshotRepository>
		<repository>
			<id>releases</id>
			<url>${releases.repository.url}</url>
		</repository>
	</distributionManagement>

</project>
